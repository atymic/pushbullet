{
    "name": "laravel-notification-channels/pushbullet",
    "description": "Laravel 5.3+ Notifications Pushbullet Driver",
    "homepage": "https://github.com/laravel-notification-channels/pushbullet",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Plekhanov",
            "email": "alex@plekhanov.us",
            "homepage": "https://alex.plekhanov.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/notifications": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.0",
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Pushbullet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Pushbullet\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Pushbullet\\PushbulletServiceProvider"
            ]
        }
    }
}
